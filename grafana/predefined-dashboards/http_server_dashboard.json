{
      "id": null,
      "uid": "minitwit_dashboard",
      "title": "HTTP Server Dashboard",
      "tags": ["http", "server", "requests", "prometheus"],
      "timezone": "browser",
      "panels": [
        {
          "type": "graph",
          "title": "HTTP Responses by Status Code",
          "targets": [
            {
              "expr": "sum(rate(http_server_requests_total{code=~\"2..\"}[1m])) by (code)",
              "interval": "",
              "legendFormat": "{{code}}",
              "refId": "A"
            },
            {
                "expr": "sum(rate(http_server_requests_total{code=~\"3..\"}[1m])) by (code)",
                "interval": "",
                "legendFormat": "{{code}}",
                "refId": "B"
              },
            {
              "expr": "sum(rate(http_server_requests_total{code=~\"4..\"}[1m])) by (code)",
              "interval": "",
              "legendFormat": "{{code}}",
              "refId": "C"
            },
            {
              "expr": "sum(rate(http_server_requests_total{code=~\"5..\"}[1m])) by (code)",
              "interval": "",
              "legendFormat": "{{code}}",
              "refId": "D"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "unit": "short",
              "color": {
                "mode": "palette-classic"
              }
            }
          },
          "legend": {
            "show": true,
            "values": false,
            "min": true,
            "max": true,
            "avg": true,
            "current": true,
            "total": false,
            "alignAsTable": false,
            "rightSide": true
          },
          "gridPos": {
            "x": 0,
            "y": 0,
            "w": 12,
            "h": 6
          }
        },
        {
          "type": "graph",
          "title": "Request Duration (ms)",
          "targets": [
            {
              "expr": "rate(http_server_request_duration_seconds_sum{method=\"get\"}[1m]) / rate(http_server_request_duration_seconds_count{method=\"get\"}[1m]) * 1000",
              "interval": "",
              "legendFormat": "GET Request Duration",
              "refId": "A"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "unit": "ms",
              "color": {
                "mode": "palette-classic"
              }
            }
          },
          "legend": {
            "show": true,
            "values": false,
            "min": false,
            "max": false,
            "avg": false,
            "current": true,
            "total": false,
            "alignAsTable": false,
            "rightSide": true
          },
          "gridPos": {
            "x": 0,
            "y": 6,
            "w": 12,
            "h": 6
          }
        },
        {
          "type": "graph",
          "title": "Request Duration for /metrics Path (ms)",
          "targets": [
            {
              "expr": "rate(http_server_request_duration_seconds_sum{path=\"/metrics\"}[1m]) / rate(http_server_request_duration_seconds_count{path=\"/metrics\"}[1m]) * 1000",
              "interval": "",
              "legendFormat": "/metrics Request Duration",
              "refId": "A"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "unit": "ms",
              "color": {
                "mode": "palette-classic"
              }
            }
          },
          "legend": {
            "show": true,
            "values": false,
            "min": false,
            "max": false,
            "avg": false,
            "current": true,
            "total": false,
            "alignAsTable": false,
            "rightSide": true
          },
          "gridPos": {
            "x": 0,
            "y": 12,
            "w": 12,
            "h": 6
          }
        }
      ],
